{
  "name": "othello",
  "version": "0.1.0",
  "private": true,
  "keywords": "othello reactjs redux",
  "homepage": "https://arthuranderson3.github.io/othello",
  "bugs": {
    "url" : "https://github.com/arthuranderson3/othello/issues"
    , "email" : "othello@arthuranderson3.github.io"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss}": [
      "node_modules/.bin/prettier --write",
      "git add ."
    ]
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "watch-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/ --watch",
    "precommit": "lint-staged",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "eslint": "./node_modules/.bin/eslint",
    "lint": "eslint src",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "bootstrap": "4.1.3",
    "classnames": "^2.2.6",
    "lodash.bindall": "^4.4.0",
    "lodash.chunk": "^4.2.0",
    "lodash.concat": "^4.5.0",
    "lodash.dropright": "^4.1.1",
    "lodash.fill": "^3.4.0",
    "lodash.filter": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "lodash.forin": "^4.4.0",
    "lodash.map": "^4.6.0",
    "lodash.reduce": "^4.6.0",
    "lodash.slice": "^4.2.0",
    "lodash.some": "^4.6.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "redux": "^3.4.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "^0.0.54",
    "uuid": "^3.3.2",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "deep-freeze": "^0.0.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages": "^2.0.1",
    "husky": "^0.14.3",
    "jest-css-modules": "^1.1.0",
    "lint-staged": "^6.1.1",
    "node-sass-chokidar": "^1.3.3",
    "prettier": "^1.9.2",
    "react-addons-test-utils": "^15.6.2",
    "redux-devtools": "^3.4.1",
    "npm-run-all": "^4.1.3"
  }
}
